{{#game}}
<ul>
    <li class="clearfix">
        <img src="{{img}}" alt="" class="more-game-img">
        <div class="more-game-box clearfix">
            <p class="more-game-name">{{name}} <span class="">独家</span></p>
            <p class="more-game-content">{{content}}</p>
        </div>
        <a href="{{url}}" class="more-game-start">开始</a>
    </li>
</ul>
{{/game}}