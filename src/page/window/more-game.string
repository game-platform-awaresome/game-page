
<ul id="moreGame">
{{#game}}
    <li class="clearfix">
        <img src="{{img}}" alt="" class="more-game-img">
        <div class="more-game-box clearfix">
            <p class="more-game-name">{{name}}</p>
            <p class="more-game-content">{{content}}</p>
        </div>
        <a data-href="{{url}}" class="more-game-start">开始</a>
    </li>
    {{/game}}
</ul>
